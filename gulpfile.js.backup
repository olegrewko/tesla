const del = require('del');

// –ü—É—Ç–∏
const paths = {
  src: {
    html: './src/**/*.html',
    scss: './src/scss/**/*.scss',
    js: './src/js/**/*.js',
    img: './src/img/**/*.{jpg,jpeg,png,svg,gif,webp}',
    svg: './src/svg/**/*.svg',
    fonts: './src/fonts/**/*.{woff,woff2,ttf,eot}',
    resources: './src/resources/**/*'
  },
  build: {
    html: './dist/',
    css: './dist/css/',
    js: './dist/js/',
    img: './dist/img/',
    fonts: './dist/fonts/',
    resources: './dist/resources/'
  },
  watch: {
    html: './src/**/*.html',
    scss: './src/scss/**/*.scss',
    js: './src/js/**/*.js',
    img: './src/img/**/*.{jpg,jpeg,png,svg,gif,webp}',
    svg: './src/svg/**/*.svg',
    fonts: './src/fonts/**/*.{woff,woff2,ttf,eot}'
  }
};

// –ò–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏—è BrowserSync
const server = browserSync.create();

// –û—á–∏—Å—Ç–∫–∞ –ø–∞–ø–∫–∏ dist
export const clean = () => {
  return del(['dist/**/*', '!dist']);
};

// –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ HTML
export const html = () => {
  return gulp.src(paths.src.html)
    .pipe(gulp.dest(paths.build.html))
    .pipe(server.stream());
};

// –ö–æ–º–ø–∏–ª—è—Ü–∏—è SCSS
export const styles = () => {
  const sass = require('gulp-sass')(require('sass'));
  const autoprefixer = require('gulp-autoprefixer');
  const cleanCSS = require('gulp-clean-css');
  const rename = require('gulp-rename');

  return gulp.src(paths.src.scss)
    .pipe(sass().on('error', sass.logError))
    .pipe(autoprefixer({
      cascade: false,
      grid: true
    }))
    .pipe(cleanCSS())
    .pipe(rename({ suffix: '.min' }))
    .pipe(gulp.dest(paths.build.css))
    .pipe(server.stream());
};

// –û–±—Ä–∞–±–æ—Ç–∫–∞ JavaScript
export const scripts = () => {
  const webpack = require('webpack-stream');
  const rename = require('gulp-rename');

  return gulp.src('./src/js/main.js')
    .pipe(webpack({
      mode: 'production',
      output: {
        filename: 'main.min.js'
      },
      module: {
        rules: [
          {
            test: /\.js$/,
            exclude: /node_modules/,
            use: {
              loader: 'babel-loader',
              options: {
                presets: ['@babel/preset-env']
              }
            }
          }
        ]
      }
    }))
    .pipe(gulp.dest(paths.build.js))
    .pipe(server.stream());
};

// –û–ø—Ç–∏–º–∏–∑–∞—Ü–∏—è –∏–∑–æ–±—Ä–∞–∂–µ–Ω–∏–π
export const images = () => {
  const imagemin = require('gulp-imagemin');

  return gulp.src(paths.src.img)
    .pipe(imagemin([
      imagemin.mozjpeg({ quality: 80, progressive: true }),
      imagemin.optipng({ optimizationLevel: 5 }),
      imagemin.svgo({
        plugins: [
          { removeViewBox: true },
          { cleanupIDs: false }
        ]
      })
    ]))
    .pipe(gulp.dest(paths.build.img))
    .pipe(server.stream());
};

// –°–æ–∑–¥–∞–Ω–∏–µ WebP
export const webp = () => {
  const webp = require('gulp-webp');

  return gulp.src('./src/img/**/*.{jpg,jpeg,png}')
    .pipe(webp({ quality: 80 }))
    .pipe(gulp.dest(paths.build.img));
};

// SVG —Å–ø—Ä–∞–π—Ç—ã
export const sprite = () => {
  const svgSprite = require('gulp-svg-sprite');

  return gulp.src(paths.src.svg)
    .pipe(svgSprite({
      mode: {
        stack: {
          sprite: '../sprite.svg'
        }
      }
    }))
    .pipe(gulp.dest(paths.build.img));
};

// –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ —à—Ä–∏—Ñ—Ç–æ–≤
export const fonts = () => {
  return gulp.src(paths.src.fonts)
    .pipe(gulp.dest(paths.build.fonts));
};

// –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ —Ä–µ—Å—É—Ä—Å–æ–≤
export const resources = () => {
  return gulp.src(paths.src.resources)
    .pipe(gulp.dest(paths.build.resources));
};

// –ù–∞–±–ª—é–¥–µ–Ω–∏–µ –∑–∞ —Ñ–∞–π–ª–∞–º–∏
export const watch = () => {
  server.init({
    server: {
      baseDir: "./dist"
    },
    notify: false,
    port: 3000,
    open: true
  });

  gulp.watch(paths.watch.html, gulp.series(html));
  gulp.watch(paths.watch.scss, gulp.series(styles));
  gulp.watch(paths.watch.js, gulp.series(scripts));
  gulp.watch(paths.watch.img, gulp.series(images));
  gulp.watch(paths.watch.svg, gulp.series(sprite));
  gulp.watch(paths.watch.fonts, gulp.series(fonts));
};

// –°–±–æ—Ä–∫–∞ –¥–ª—è —Ä–∞–∑—Ä–∞–±–æ—Ç–∫–∏
export const dev = gulp.series(
  clean,
  gulp.parallel(
    html,
    styles,
    scripts,
    images,
    webp,
    sprite,
    fonts,
    resources
  ),
  watch
);

// –°–±–æ—Ä–∫–∞ –¥–ª—è –ø—Ä–æ–¥–∞–∫—à–µ–Ω–∞
export const build = gulp.series(
  clean,
  gulp.parallel(
    html,
    styles,
    scripts,
    images,
    webp,
    sprite,
    fonts,
    resources
  )
);

// –°–æ–∑–¥–∞–Ω–∏–µ ZIP-–∞—Ä—Ö–∏–≤–∞
export const zip = () => {
  const zip = require('gulp-zip');

  return gulp.src('dist/**/*')
    .pipe(zip('project.zip'))
    .pipe(gulp.dest('./'));
};

// –≠–∫—Å–ø–æ—Ä—Ç –∑–∞–¥–∞—á –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é
export default dev;
